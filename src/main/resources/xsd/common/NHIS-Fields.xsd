<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:nhis="https://www.his.bg" targetNamespace="https://www.his.bg"
	elementFormDefault="qualified" version="1.0.4">
	<xs:include schemaLocation="./NHIS-Commons.xsd" />
	<!-- Define field base types for reuse in multiple messages. WARNING: Keep 
		the alphabetical order -->
	<xs:complexType name="accessTokenBase">
		<xs:annotation>
			<xs:documentation>Токен за достъп до НЗИС</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:string"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="addressDetailsBase">
		<xs:annotation>
			<xs:documentation>Пълен адрес</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="2000" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="ageBase">
		<xs:annotation>
			<xs:documentation>Възраст</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:positiveInteger">
					<xs:totalDigits value="3" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="allowedRepeatsNumberBase">
		<xs:annotation>
			<xs:documentation>Колко пъти дадена рецептурна бланка за многократно
				отпускане може да се изпълнява. За неограничен брой пъти се въвежда
				0</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:unsignedInt">
					<xs:totalDigits value="1" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="asNeededBase">
		<xs:annotation>
			<xs:documentation>Прием на лекарството при необходимост
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="authoredOnBase">
		<xs:annotation>
			<xs:documentation>Дата на издаване на документ</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:date" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="birthDateBase">
		<xs:annotation>
			<xs:documentation>Дата на раждане</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:date" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="boundsDurationBase">
		<xs:annotation>
			<xs:documentation>Продължителност на изпълнението на инструкциите в
				посочената мерна единица (напр. 30 дни, 2 седмици)
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:positiveInteger"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="boundsDurationUnitBase">
		<xs:annotation>
			<xs:documentation>Мерна единица за продължителността на изпълнението
				на инструкциите</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL020"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="cityBase">
		<xs:annotation>
			<xs:documentation>Град по настоящ адрес</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphaString">
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="countryCodeBase">
		<xs:annotation>
			<xs:documentation>Kод на държава/Националност по ISO 3166-1 alpha-2
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL005"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="deputyRoleBase">
		<xs:annotation>
			<xs:documentation>Роля по заместване на медицински специалист
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL023"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="dispensationTypeBase">
		<xs:annotation>
			<xs:documentation>Начин на употреба на Е-рецепта от тип Т1
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL008"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="doseQuantityBase">
		<xs:annotation>
			<xs:documentation>Количество на медикамента за еднократен прием
				(спрямо инструкциите за дозировка). Например: 2 таблетки, 1 капка, 4
				впръсквания</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:decimal"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="emailBase">
		<xs:annotation>
			<xs:documentation>Имейл за връзка</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[^@]+@[^\.]+\..+" />
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="executeDateBase">
		<xs:annotation>
			<xs:documentation>Дата на завършване на дейностите</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:date" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="expBase">
		<xs:annotation>
			<xs:documentation>Срок на Годност на Лекарствен Продукт
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:numericString">
					<xs:length value="6" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="expiresInBase">
		<xs:annotation>
			<xs:documentation>Време, в което валидността на ресурс изтича (в
				секунди)</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:positiveInteger"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="expiresOnBase">
		<xs:annotation>
			<xs:documentation>Дата и час, в които валидността на ресурс изтича
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:dateTime"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="familyNameBase">
		<xs:annotation>
			<xs:documentation>Фамилия/Последно име</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphaString">
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="medicationFormBase">
		<xs:annotation>
			<xs:documentation>Лекарствена форма</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL010"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="frequencyBase">
		<xs:annotation>
			<xs:documentation>Честота на изпълнение на инструкциите спрямо
				периода на изпълнение</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:positiveInteger"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="fulfilDateBase">
		<xs:annotation>
			<xs:documentation>Дата на изпълнение</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:date" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="genderBase">
		<xs:annotation>
			<xs:documentation>Пол</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL001"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="givenNameBase">
		<xs:annotation>
			<xs:documentation>Собствено име</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphaString">
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="groupIdentifierBase">
		<xs:annotation>
			<xs:documentation>Само при рецепти от тип 5а - отрязък от
				рецептурната бланка (А, B или C), към който спада този медикамент
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL019"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="gtinBase">
		<xs:annotation>
			<xs:documentation>Национален Търговски Номер на Лекарствен Продукт
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:numericString">
					<xs:length value="14" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="identifierBase">
		<xs:annotation>
			<xs:documentation>В зависимост от identifierType, има различно
				значение, съдържание и структура</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphanumericString">
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="identifierTypeBase">
		<xs:annotation>
			<xs:documentation>Тип на идентификатор - ЕГН/ЛНЧ/SSN/Паспорт/Друг
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL004"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isBreastFeedingBase">
		<xs:annotation>
			<xs:documentation>Индикация дали пациентката е кърмачка
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isCorrectionBase">
		<xs:annotation>
			<xs:documentation>Указва дали заявката е коригираща заявка за
				предходно въведени резултати</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isDispensedBase">
		<xs:annotation>
			<xs:documentation>Уакзва дали този медикамент е вече издаден
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isFulfilledBase">
		<xs:annotation>
			<xs:documentation>Уакзва дали рецептата е изпълнена (т.е. всички
				медикаменти са издадени към пациента)</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isInternationalFulfilBase">
		<xs:annotation>
			<xs:documentation>Дали рецептата е изпълнена извън България/Страната
				на издаване</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isPatientReceiverBase">
		<xs:annotation>
			<xs:documentation>Указва дали получателят на медикамента е самия
				пациент</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isPregnantBase">
		<xs:annotation>
			<xs:documentation>Индикация дали пациентката е бременна
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isProcessingBase">
		<xs:annotation>
			<xs:documentation>Указва дали лабораторията започва работа по тази
				дейност (напр. след събрана проба, направена снимка)
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isProtocolBasedBase">
		<xs:annotation>
			<xs:documentation>Указва дали медикаментите ще се изписват по
				протокол или не</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isQuantityByFormBase">
		<xs:annotation>
			<xs:documentation>Указва дали предписаното количество е по опаковки
				или по лекарствена форма</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="isSubstitutionAllowedBase">
		<xs:annotation>
			<xs:documentation>Указва дали могат да бъдат изписани заместители за
				съответното лекарство</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:boolean"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="issuedOnBase">
		<xs:annotation>
			<xs:documentation>Дата и час, в които ресурс е издаден
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:dateTime"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="lrnBase">
		<xs:annotation>
			<xs:documentation>Уникален код генериран от системата на
				лекаря/стоматолога. Например: Година + УИН + пореден номер за
				съответната година</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphanumericString">
					<xs:maxLength value="36" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="lotBase">
		<xs:annotation>
			<xs:documentation>Номер на Партида на Лекарствен Продукт
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphanumericString">
					<xs:minLength value="1" />
					<xs:maxLength value="22" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="mdaBase">
		<xs:annotation>
			<xs:documentation>МКБ код</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL022"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="medicationCodeBase">
		<xs:annotation>
			<xs:documentation>Уникален идентификатор на медикамент. Извлича се от
				регистрите на НСЦРЛП</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL009"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="middleNameBase">
		<xs:annotation>
			<xs:documentation>Второ/Презиме/Бащино име (при повече от едно
				междинно име, въвеждат се в това поле разделени с интервал)
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphaString">
					<xs:maxLength value="255" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="mkbBase">
		<xs:annotation>
			<xs:documentation>МКБ код</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL011"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="nhifCodeBase">
		<xs:annotation>
			<xs:documentation>НЗОК код на Лекарствен продукт</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL026"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="nhifNumberBase">
		<xs:annotation>
			<xs:documentation>НЗОК номер на който е регистрирана съответната
				практика</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:string"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="nrnBase">
		<xs:annotation>
			<xs:documentation>Национален уникален номер на медицински документ,
				генериран от НЗИС</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphanumericString">
					<xs:length value="12" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="observationCodeBase">
		<xs:annotation>
			<xs:documentation>Тип на наблюдението</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="nhis:CL024"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="offsetBase">
		<xs:annotation>
			<xs:documentation>Интервал в минути спрямо времето или събитието,
				според което да се приеме лекарството (напр. "25 минути преди сън")
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="xs:unsignedInt"
			use="required" />
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="outpatientSheetNumberBase">
		<xs:annotation>
			<xs:documentation>Номер на амбулаторен лист, за който се изписва
				рецептата</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="nhis:alphanumericString">
					<xs:maxLength value="12" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dataType" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="performedDateTimeBase">
		<xs:annotation>
			<xs:documentation>Дата, на която от пациента е взет биологичен
				материал или е извършено образно изследване</xs:documentation>
		</xs:annotation>
